{
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx"
  ],
  "extension": [
    ".ts",
    ".tsx"
  ],
  "require": [
    "ts-node/register"
  ],
  "reporter": [
    "text-summary"
  ],
  "sourceMap": true,
  "instrument": true,
  "check-coverage": true,

  "statements": 90,
  "branches": 90,
  "lines": 90,
  "functions": 90
}
